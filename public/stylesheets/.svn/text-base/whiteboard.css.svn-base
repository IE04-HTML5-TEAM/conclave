.toolbar ul {
  margin:10px;
}
.toolbar li {
  width:44px;
  height:44px;
  margin:auto;
  list-style-type: none;
  border:1px solid #ccc;
  border-radius:6px;
  display:block;
  background: white;
  float:left;
}

