  margin:0px;  
}

ul.slideToolbar {
  margin:5px;
}

div.slideToolbar {
  min-width:60px;
  min-height:60px;;
  margin:5px 0px 5px 5px;
  display:block;
  float:left;
  position:relative;
}

ul.normalMenu {
  height:68px;
  margin:0px;  
  padding:0px;
  border:1px solid #ccc;
}

.normalMenu li {
  width:44px;
  height:44px;
  margin:auto;
  list-style-type: none;
  border:1px solid #ccc;
  display:block;
  float:left;
  background-color:white;
}

ul.slideMenu {  
  margin:0px;
  padding:0px;
  z-index:10;
  position:absolute;
  border:1px solid #ccc;
  background-color:white;
}
.slideMenu li {
  width:44px;
  height:44px;
  display:none;
  margin:0px;
  padding:0px;
  list-style-type: none;
  border:1px solid #ccc;
  margin-left:1px;
  background-color:white;
}

.slideMenu li:hover, .normalMenu li:hover, .toolbar li:hover{
  box-shadow: 0px 0px 7px 2px #F7816A; 
}

li.selected {
 border: solid 2px #36DB11;
 border-radius: 1px;
 box-shadow: 0px 0px 7px 2px #8AF96B; 
 margin-left:0px;
